  "bugs": {
    "url": "https://github.com/ahmetyesil/youtube-watch-plugin/issues"
  },
  "homepage": "https://github.com/ahmetyesil/youtube-watch-plugin#readme",
  "dependencies": {
    "animate.css": "^3.6.1",
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "jquery": ">=1.6",
    "jquery-rest-client": "^0.3.7",
    "jquery-ui": "^1.12.1",
    "jquery-ui-dist": "^1.12.1",
    "jquery-validation": "^1.18.0",
    "normalize.css": "^8.0.0",
    "querystring": "^0.2.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.7.8",
    "uglify-es": "^3.3.2",
    "zip-dir": "^1.0.2",
    "json-minify": "^1.0.0"
  },
